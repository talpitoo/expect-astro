{logos.map(logo => (
  <img
    data-speed={logo.speed}
    src={`${logo.src}?ver=28082025`}
    alt={logo.name}
    class={logo.class}
  />
))}

<style>
  .logo-smart {
    width: auto;
    height: auto;
    max-width: clamp(32px, 160px, 200px);
    max-height: clamp(32px, 55px, 80px);
    object-fit: contain;
  }
</style>